<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<!-- 修改：jdbc jar包 -->
	<!--<classPathEntry location="C:/Program Files (x86)/MySQL/MySQL Tools for 5.0/java/lib/mysql-connector-java-5.0.4-bin.jar" />-->
	<classPathEntry
		location="/Users/XJD/360yunpan/repo/maven_repository/mysql/mysql-connector-java/5.1.21/mysql-connector-java-5.1.21.jar"/>
	<!-- 生成的class所在的根路径 -->
	<classPathEntry location="target/classes" />

	<context id="common" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<!-- <property name="suppressDate" value="true" /> -->
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/ct"
			userId="root" password="root" />

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.xjd.ct.dal.model" targetProject="MAVEN">
			<property name="enableSubPackages" value="true" />
			<property name="constructorBased" value="false" />
			<property name="rootClass" value="com.xjd.ct.dal.model.BaseDo"/>
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.xjd.ct.dal.mapper" targetProject="MAVEN">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER" targetPackage="com.xjd.ct.dal.mapper" targetProject="MAVEN">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>


		<table tableName="t_article" domainObjectName="ArticleDo"/>
		<table tableName="t_article_comment" domainObjectName="ArticleCommentDo"/>
		<table tableName="t_article_comment_summary" domainObjectName="ArticleCommentSummaryDo"/>
		<table tableName="t_article_like" domainObjectName="ArticleLikeDo"/>
		<table tableName="t_article_liker" domainObjectName="ArticleLikerDo"/>
		<table tableName="t_article_tag" domainObjectName="ArticleTagDo"/>
		<table tableName="t_article_vote" domainObjectName="ArticleVoteDo"/>
		<table tableName="t_article_voter" domainObjectName="ArticleVoterDo"/>
		<table tableName="t_children_title" domainObjectName="ChildrenTitleDo"/>
		<table tableName="t_rs_article_tag" domainObjectName="RsArticleTagDo"/>
		<table tableName="t_service" domainObjectName="ServiceDo"/>
		<table tableName="t_service_log" domainObjectName="ServiceLogDo"/>
		<table tableName="t_token" domainObjectName="TokenDo"/>
		<!--<table tableName="t_user" domainObjectName="UserDo">-->
			<!--<overide-->
		<!--</table>-->

	</context>

</generatorConfiguration>